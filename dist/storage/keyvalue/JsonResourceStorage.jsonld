          "range": "xsd:string",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/storage/keyvalue/JsonResourceStorage#JsonResourceStorage_source",
          "range": "scs:dist/storage/ResourceStore#ResourceStore",
          "unique": true,
          "required": true
        }
      ],
      "constructorArguments": [
        {
          "@id": "scs:dist/storage/keyvalue/JsonResourceStorage#JsonResourceStorage_source"
