{
  "@context": [
    "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^2.0.0/components/context.jsonld"
  ],
  "@id": "npmd:@solid/community-server",
  "components": [
    {
      "@id": "scs:dist/storage/accessors/SparqlDataAccessor#SparqlDataAccessor",
      "@type": "Class",
      "requireElement": "SparqlDataAccessor",
      "extends": [
        "scs:dist/storage/accessors/DataAccessor#DataAccessor"
      ],
      "comment": "Stores all data and metadata of resources in a SPARQL backend. Communication is done by sending SPARQL queries. Queries are constructed in such a way to keep everything consistent, such as updating containment triples and deleting old data when it is overwritten.  Since metadata is hidden, no containment triples are stored for metadata files.  All input container metadata is stored in its metadata identifier. The containment triples are stored in the graph corresponding to the actual identifier so those don't get overwritten.",
      "parameters": [
        {
          "@id": "scs:dist/storage/accessors/SparqlDataAccessor#SparqlDataAccessor_endpoint",
          "range": "xsd:string",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/storage/accessors/SparqlDataAccessor#SparqlDataAccessor_identifierStrategy",
          "range": "scs:dist/util/identifiers/IdentifierStrategy#IdentifierStrategy",
          "unique": true,
          "required": true
        }
      ],
      "constructorArguments": [
        {
          "@id": "scs:dist/storage/accessors/SparqlDataAccessor#SparqlDataAccessor_endpoint"
        },
        {
          "@id": "scs:dist/storage/accessors/SparqlDataAccessor#SparqlDataAccessor_identifierStrategy"
        }
      ]
    }
  ]
}