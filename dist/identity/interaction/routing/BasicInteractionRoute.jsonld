        "scs:dist/identity/interaction/routing/InteractionRoute#InteractionRoute"
      ],
      "comment": "Default implementation of the InteractionRoute. See function comments for specifics.",
      "parameters": [
        {
          "@id": "scs:dist/identity/interaction/routing/BasicInteractionRoute#BasicInteractionRoute_route",
          "range": "xsd:string",
          "comment": "Regex to match this route.",
          "unique": true,
          "required": true
        },
        {
