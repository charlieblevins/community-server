        {
          "@id": "scs:dist/identity/interaction/routing/BasicInteractionRoute#BasicInteractionRoute_controls",
          "range": {
            "@type": "scs:dist/identity/interaction/routing/BasicInteractionRoute#BasicInteractionRoute_controls_range",
