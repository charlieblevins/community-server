          "range": {
            "@type": "scs:dist/identity/interaction/routing/BasicInteractionRoute#BasicInteractionRoute_viewTemplates_range",
            "parameters": [
              {
                "@id": "scs:dist/identity/interaction/routing/BasicInteractionRoute#BasicInteractionRoute_viewTemplates_key",
                "required": true,
                "unique": true
