          "range": "scs:dist/identity/interaction/util/InteractionCompleter#InteractionCompleter",
          "comment": "Used for POST requests that need to be handled by the OIDC library.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_errorHandler",
          "range": "scs:dist/http/output/error/ErrorHandler#ErrorHandler",
