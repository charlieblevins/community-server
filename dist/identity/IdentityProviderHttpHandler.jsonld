  "components": [
    {
      "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler",
      "@type": "Class",
      "requireElement": "IdentityProviderHttpHandler",
      "extends": [
        "scs:dist/server/OperationHttpHandler#OperationHttpHandler"
      ],
      "comment": "Handles all requests relevant for the entire IDP interaction, by sending them to either a matching {@link InteractionRoute}, or the generated Provider from the {@link ProviderFactory} if there is no match.  The InteractionRoutes handle all requests where we need custom behaviour, such as everything related to generating and validating an account. The Provider handles all the default request such as the initial handshake.  This handler handles all requests since it assumes all those requests are relevant for the IDP interaction. A {@link RouterHandler} should be used to filter out other requests.",
      "parameters": [
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_baseUrl",
          "range": "xsd:string",
          "comment": "Base URL of the server.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_idpPath",
          "range": "xsd:string",
          "comment": "Relative path of the IDP entry point.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_providerFactory",
          "range": "scs:dist/identity/configuration/ProviderFactory#ProviderFactory",
          "comment": "Used to generate the OIDC provider.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_interactionRoutes",
          "range": "scs:dist/identity/interaction/routing/InteractionRoute#InteractionRoute",
          "comment": "All routes handling the custom IDP behaviour.",
          "required": true
        },
        {
          "@id": "scs:dist/identity/IdentityProviderHttpHandler#IdentityProviderHttpHandler_args_converter",
          "range": "scs:dist/storage/conversion/RepresentationConverter#RepresentationConverter",
          "comment": "Used for content negotiation.",
          "unique": true,
          "required": true
        },
        {
